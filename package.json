{
  "name": "@yukiyuriweb/ampcss",
  "version": "5.1.2",
  "description": "Low-level CSS toolkit for AMP HTML",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yukiyuriweb/ampcss.git"
  },
  "scripts": {
    "compile": "sass ampcss.scss dist/ampcss.css",
    "build": "sass ampcss.scss dist/ampcss.css --style compressed; AUTOPREFIXER_GRID=autoplace postcss --no-map dist/ampcss.css --use autoprefixer -o dist/ampcss.css"
  },
  "author": "YUKiYURi WEB <info@yukiyuriweb.com>",
  "license": "MIT",
  "dependencies": {},
  "keywords": [
    "css",
    "framework",
    "basscss",
    "amp",
    "accelerated mobile pages"
  ],
  "bugs": {
    "url": "https://github.com/yukiyuriweb/ampcss/issues"
  },
  "homepage": "https://github.com/yukiyuriweb/ampcss",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "dart-sass": "^1.25.0",
    "postcss-cli": "^7.1.2"
  }
}
